BEGIN
    READ L1, B1, L2, B2

    area1 = L1 * B1
    area2 = L2 * B2

    perimeter1 = 2 * (L1 + B1)
    perimeter2 = 2 * (L2 + B2)

    IF area1 > area2 THEN
        PRINT "true"
    ELSE
        PRINT "false"
    ENDIF

    IF perimeter1 > perimeter2 THEN
        PRINT "true"
    ELSE
        PRINT "false"
    ENDIF
END
